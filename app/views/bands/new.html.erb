<h1>New band</h1>

<form action="<%= bands_url %>" method="post">
  <%= auth_token %>
  <%= render "shared/errors" if flash[:errors] %>

  <br>
  <label for="name">Name</label>
  <input type="text" name="bands[name]" id="name" value="<%= @band.name %>">
  <br>

  <input type="submit" value="make band">
</form>

<br>
<%= link_to "back", :back %>
